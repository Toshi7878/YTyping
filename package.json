  "name": "ytyping",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate || exit 0",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --cache",
    "typecheck": "tsc --noEmit",
    "reset": "rm -rf .next .tsbuildinfo node_modules package-lock.json && npm install",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev --name init",
    "studio": "dotenv -e .env.local -- npx prisma studio",
    "db:generate": "dotenv -e .env.local -- npx prisma generate",
    "db:reset": "dotenv -e .env.local -- npx prisma migrate reset"
